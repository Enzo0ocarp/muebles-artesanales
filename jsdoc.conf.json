{
  "source": {
    "include": ["src/"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/", "build/", "src/reportWebVitals.js", "src/setupTests.js"]
  },
  "opts": {
    "destination": "docs/",
    "recurse": true,
    "readme": "README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}